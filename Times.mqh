#include <Arrays\ArrayObj.mqh>
#include "Time.mqh"
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
class JTimes : public CArrayObj
  {
private:

public:
                     JTimes();
                    ~JTimes();
   virtual bool      Evaluate();
  };
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
JTimes::JTimes()
  {
  }
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
JTimes::~JTimes()
  {
  }
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
bool JTimes::Evaluate()
  {
   for(int i=0;i<Total();i++)
     {
      JTime *time=At(i);
      if(CheckPointer(time))
         if (!time.Evaluate()) return(false);
     }
    return(true);
  }
